<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>mvc2_1</display-name>
	<welcome-file-list>
		<welcome-file>/views/main.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>test</servlet-name>
		<servlet-class>controller.TestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>main</servlet-name>
		<jsp-file>/views/main.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>join</servlet-name>
		<jsp-file>/views/join.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/views/login.jsp</jsp-file>
	</servlet>
	
	
